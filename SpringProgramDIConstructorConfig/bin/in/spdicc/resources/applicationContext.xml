<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

		<bean class="in.spdicc.beans.Address" id="addrId">
			<constructor-arg value="24"></constructor-arg>
			<constructor-arg value="Delhi"></constructor-arg>
			<constructor-arg value="110001"></constructor-arg>
		</bean>

    	<bean class="in.spdicc.beans.Student" id="stdId">
			<constructor-arg value="Mayank"></constructor-arg>
			<constructor-arg value="27"></constructor-arg>
			<constructor-arg ref="addrId"/>
		</bean>

</beans>